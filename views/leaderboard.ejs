<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8" />
  <title>Top 10</title>
  <link rel="stylesheet" href="/css/style.css" />
</head>
<body>
<%- include('partials/header', { user: user }) %>
<div class="centered">
  <h2>TOP 10 - Reichste Spieler</h2>
  <table>
    <tr><th>Platz</th><th>Name</th><th>Coins</th><th>Level</th></tr>
    <% users.forEach((u,i) => { %>
      <tr>
        <td><%= i+1 %></td><td><%= u.username %></td><td><%= u.coins %></td><td><%= u.level %></td>
      </tr>
    <% }); %>
  </table>
</div>
<%- include('partials/footer') %>
</body>
</html>